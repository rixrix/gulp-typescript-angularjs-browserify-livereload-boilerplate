{
  "name": "stencil",
  "version": "1.1.0",
  "description": "A horribly simple starter template for all your Angular2 frontend development",
  "scripts": {
    "release": "./node_modules/.bin/standard-version && ./node_modules/.bin/conventional-github-releaser -p angular -t $CONVENTIONAL_GITHUB_RELEASER_TOKEN",
    "build": "./node_modules/.bin/rimraf dist/*.* && npm run build:prod",
    "build:prod": "./node_modules/.bin/webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "./node_modules/.bin/webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --host=0.0.0.0 --content-base app/",
    "start": "npm run server"
  },
  "authors": [
    "Richard Sentino <richard@mindginative.com>",
    "Franco Sioquim <francosioquim@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rixrix/stencil.git"
  },
  "homepage": "https://github.com/rixrix/stencil",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
